--Properties:

MeleGui.Name = "Mele Gui"
MeleGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
MeleGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = MeleGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.Position = UDim2.new(0.730247378, 0, 0.248861909, 0)
Frame.Size = UDim2.new(0, 302, 0, 228)
Frame.Style = Enum.FrameStyle.DropShadow
Frame.Active = true
Frame.Draggable = true

TextLabel.Parent = Frame
