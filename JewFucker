-- Gui to Lua
-- Version: 3.2

-- Instances:

local MeleGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local Player = Instance.new("TextButton")
local FE = Instance.new("TextButton")
